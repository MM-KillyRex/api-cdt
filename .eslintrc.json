{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-async-promise-executor": "off",
    "no-underscore-dangle": "off"
  },
  "globals": {
    "__base": "readonly"
  }
}